{
  "name": "translime-plugin-namesilo-dns",
  "version": "1.0.0",
  "description": "translime plugin",
  "license": "MIT",
  "keywords": [
    "translime",
    "translime-plugin"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vite -c ui.vite.config.js",
    "build:plugin": "vite build",
    "build:ui": "vite build -c ui.vite.config.js",
    "build": "yarn build:plugin && yarn build:ui"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.1.7",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-preact": "^1.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "2.25.2",
    "jest": "^27.5.1",
    "vite": "^2.7.2"
  },
  "plugin": {
    "title": "namesilo dns",
    "description": "定时获取ip并设定为namesilo的域名解析",
    "windowUrl": "http://localhost:3000/ui.html"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "fast-xml-parser": "^4.0.7",
    "preact": "^10.6.6"
  }
}
